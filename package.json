{
	"name": "catppuccinify",
	"version": "0.1.0",
	"scripts": {
		"next:dev": "next dev",
		"tw:dev": "npx tailwindcss -i ./src/styles/tw.css -o ./src/styles/globals.css --watch",
		"dev": "run-p tw:dev next:dev",
		"build": "npx tailwindcss -i ./src/styles/tw.css -o ./src/styles/globals.css --minify && next build",
		"fmt": "npx prettier --write src",
		"start": "next start",
		"lint": "next lint",
		"export": "pnpm build && next export && ./build.sh"
	},
	"dependencies": {
		"file-saver": "^2.0.5",
		"next": "13.2.4",
		"nprogress": "^0.2.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"wasm-imagemagick": "^1.2.8"
	},
	"devDependencies": {
		"@catppuccin/tailwindcss": "^0.1.1",
		"@types/node": "18.15.11",
		"@types/nprogress": "^0.2.0",
		"@types/react": "18.0.32",
		"@types/react-dom": "18.0.11",
		"eslint": "8.37.0",
		"eslint-config-next": "13.2.4",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.7",
		"tailwindcss": "^3.3.1",
		"typescript": "5.0.3"
	}
}
